{
  "name": "electronapp",
  "productName": "electronapp",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "electron.js",
  "scripts": {
    "start": "npm install && electron electron.js --no-sandbox"
  },
  "keywords": [],
  "author": {
    "name": "Yassa Taiseer",
    "email": "yassataiseer@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
